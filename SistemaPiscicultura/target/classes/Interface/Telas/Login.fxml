<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Interface.Telas.LoginController">
    <Button fx:id="btnLogin" layoutX="140.0" layoutY="230.0" mnemonicParsing="false" onAction="#Login" text="Login">
        <font>
            <Font size="18.0" />
        </font>
    </Button>
    <TextField fx:id="txtUsername" layoutX="63.0" layoutY="80.0" promptText="Username">
        <font>
            <Font size="18.0" />
        </font>
    </TextField>
    <PasswordField fx:id="txtPassword" layoutX="63.0" layoutY="154.0" promptText="Password">
        <font>
            <Font size="18.0" />
        </font>
    </PasswordField>
    <Label fx:id="lblStatus" contentDisplay="CENTER" layoutX="80.0" layoutY="19.0" prefHeight="27.0" prefWidth="191.0" textAlignment="CENTER" wrapText="true">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
</AnchorPane>
